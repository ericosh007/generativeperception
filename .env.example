# GPL Environment Configuration

# Application
GPL_DEBUG=false
GPL_LOG_LEVEL=INFO

# Processing
GPL_MODE=adaptive
GPL_TARGET_FPS=30
GPL_HDR_ALGO=advanced_clahe
GPL_HDR_PRESET=balanced
GPL_HDR_NITS=1000
GPL_USE_GPU=true
GPL_USE_NEURAL=true

# Telemetry
GPL_TELEMETRY_SOURCES=system,simulated
GPL_TELEMETRY_HZ=10.0
GPL_TELEMETRY_BUFFER=1000
GPL_MOTION_THRESHOLD=0.3

# Web Server
GPL_HOST=0.0.0.0
GPL_PORT=8000
GPL_CORS_ORIGINS=*

# Streaming
GPL_STREAM_PROTOCOL=webrtc
GPL_STREAM_BITRATE=8000000
GPL_STREAM_CODEC=h264
GPL_LOW_LATENCY=true

# Redis
GPL_REDIS_URL=redis://localhost:6379
GPL_CACHE_TTL=3600

# Monitoring
GPL_METRICS=true
GPL_METRICS_PORT=9090

# Paths
GPL_DATA_DIR=./data
GPL_MODEL_DIR=./models
GPL_OUTPUT_DIR=./data/output